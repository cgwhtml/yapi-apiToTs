{
    "name": "yapi-apiToTs",
    "displayName": "yapi-apiToTs",
    "description": "api文档转ts的vscode插件",
    "version": "1.0.6",
    "publisher": "chengw",
    "engines": {
        "vscode": "^1.60.0"
    },
    "activationEvents": [
        "onCommand:extension.apiToTs"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.apiToTs",
                "title": "apiToTs"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "extension.apiToTs",
                    "group": "navigation",
                    "when": "explorerResourceIsFolder"
                }
            ]
        }
    }, 
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "^14.14.37",
        "@types/vscode": "^1.60.0",
        "@vscode/test-electron": "^2.3.2",
        "cheerio": "^1.0.0-rc.12",
        "eslint": "^8.41.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "puppeteer": "^22.12.1",
        "source-map-loader": "^5.0.0",
        "ts-loader": "^9.5.1",
        "typescript": "^5.5.3",
        "vscode": "^1.1.37",
        "vscode-test": "^1.4.0",
        "webpack": "^5.92.1",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {
        "fs": "^0.0.1-security",
        "path": "^0.12.7"
    }
}
